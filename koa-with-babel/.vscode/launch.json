  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序",
      "program": "${workspaceFolder}\\index.js",
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": ["${workspaceRoot}\\node_modules\\**\\*.js", "<node_internals>\\**\\*.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动程序builded",
      "program": "${workspaceFolder}\\build\\index.js",
      "sourceMaps": true,
      "smartStep": true,
      "outFiles": ["${workspaceRoot}\\build\\**\\*.js"],
      "skipFiles": ["${workspaceRoot}\\node_modules\\**\\*.js", "<node_internals>\\**\\*.js"]
    }
  ]
}
